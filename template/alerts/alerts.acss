
.mask{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6)
}
.mbox{
  width: 500rpx;
  background: #fff;
  border-radius: 15rpx;
  padding:38rpx 24rpx 38rpx;
  position: fixed;
  top:50%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.mtit{
  font-size: 38rpx;
  color: #000;

}
.mcont{
  font-size: 30rpx;
  color: #333;
  margin-top: 20rpx
}